# Environment Configuration Example
# Copy this file to .env.local and update the values for your local development

# API Configuration
VITE_API_URL=http://localhost:8000/api/v1

# Application Environment
VITE_APP_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_SERVICE_WORKER=false
VITE_ENABLE_CACHING=false
VITE_ENABLE_CSP=false

# Logging
VITE_LOG_LEVEL=debug

# Debug Flags (development only)
VITE_DEBUG_API=true
VITE_DEBUG_REDUX=false

# ============================================
# PRODUCTION EXAMPLE (for .env.production):
# ============================================
# VITE_API_URL=https://api.yourapp.com/api/v1
# VITE_APP_ENV=production
# VITE_ENABLE_ANALYTICS=true
# VITE_ENABLE_SERVICE_WORKER=true
# VITE_ENABLE_CACHING=true
# VITE_ENABLE_CSP=true
# VITE_LOG_LEVEL=error
# VITE_DEBUG_API=false
# VITE_DEBUG_REDUX=false